package oddeven;

import java.util.Scanner;

public class Oddoreven {

	public static void main(String[] args) {
		System.out.print("enter number: ");
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		
		if (n%2==0)
          System.out.println("even");
		else
			System.out.println("odd");
		
		

	}

}
