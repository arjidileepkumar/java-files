package mypackage;
import java.util.*;
public class Myclass {

	public static void main(String[] args) {
		// to make any exersice
	System.out.print("your number");
	Scanner s = new Scanner(System.in);
	int n = s.nextInt();
	if ( n % 2 == 0)
		System.out.println("even");
	
	if ( n % 2 ==1)
		System.out.println("odd");
	
	}

}
