package calcul;

import java.util.Scanner;

public class Calcultor {

	public static void main(String[] args) {
		
		Scanner s= new Scanner (System.in);
		int d= s.nextInt();
		char op = s.next().charAt(0);
		int f = s.nextInt();
		System.out.println("enter forst number: "+d);
		System.out.println("entet another number: "+f);
		System.out.println("enter the symbol: "+op);
		if (op=='+')
			System.out.println("result is: "+(d+f));
		else if(op=='-')
			System.out.println("resuts is: "+(d-f));
		else if (op=='*')
			System.out.println("results is: "+(d*f));
		else if(op=='/')
			System.out.println("results is: "+(d/f));
		
			}
		
		
	

	}


